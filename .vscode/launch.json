  "version": "0.2.0",
  "configurations": [
    {
      "name": "Default",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
    },
    {
      "name": "Command: Version",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": ["version"]
    },
    {
      "name": "Command: Pods",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": ["pods"]
    },
    {
      "name": "Command: Create Project",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": ["create", "project", "pippo", "--display-name", "pippo", "--cluster-name", "local"]
    },
    {
      "name": "Command: Delete Project",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": ["delete", "project", "pippo", "--cluster-name", "local"]
    },
    {
      "name": "Command: Get Projects",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": ["get", "project", "--cluster-name", "local"]
    },
    {
      "name": "Command: Get Project",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": ["get", "project", "p-xs2md", "--cluster-name", "local"]
    },
    {
      "name": "Generate error",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": ["create", "foo"]
    },
    {
      "name": "Trace log level",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": ["-v", "16", "get", "project", "--cluster-name", "local"]
    }
  ]
}